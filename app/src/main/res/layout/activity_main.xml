                app:backgroundTint="#202020" />

            <View
                android:id="@+id/view11"
                android:layout_width="match_parent"
                android:layout_height="3dp"
                android:background="#FF3838"
                app:layout_constraintBottom_toTopOf="@+id/scrollView2"
